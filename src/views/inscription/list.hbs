<div class="container">
	<div class="col-md-4 mx-auto">
		<h1>
			Inscripciones Registrados
		</h1>
	</div>
</div>
<br />
<div class="row">
	<table class="table table-striped table-hover">
		<thead>
		<tr>
			<th scope="col" >Id</th>
			<th scope="col " >Proyecto</th>
			<th scope="col ">Usuario</th>
			<th scope="col ">Propuesta</th>
			<th scope="col ">Estado</th>
		</tr>
		</thead>

		{{#each dataRender}}
			<tbody>
			<tr>
				<td>
					{{projectId}}
				</td>
				</th>
				<td>
					{{projectName}}
				</td>
				<td>
					{{username}}
				</td>
				<td>
					{{description}}
				</td>
				<td>
					{{state}}
				</td>
				<td >
					<form action="/inscription/approve/{{_id}}" method="post">
						<button type="submit" class="btn btn-primary btn-sm">Aprobar</button>
					</form>
				</td>
				<td >
					<form action="/inscription/delete/{{_id}}" method="post">
						<button type="submit" class="btn btn-danger btn-sm">Borrar</button>
					</form>
				</td>
			</tr>
			</tbody>

		{{/each}}
	</table>
</div>
