<div class="container">
	<div class="col-md-4 mx-auto">
		<h1>
			Proyectos Registrados
		</h1>
	</div>
</div>
<br />
<div class="row">
	<table class="table table-striped table-hover">
		<thead>
		<tr>
			<th scope="col" >Id</th>
			<th scope="col " >Proyecto</th>
			<th scope="col ">Usuario</th>
			<th scope="col ">Propuesta</th>
			<th scope="col ">Estado</th>
			<th scope="col ">Fase</th>
		</tr>
		</thead>

		{{#each dataRender}}
			<tbody>
			<tr>
				<td>
					{{projectId}}
				</td>
				</th>
				<td>
					{{projectName}}
				</td>
				<td>
					{{username}}
				</td>
				<td>
					{{description}}
				</td>
				<td>
					{{state}}
				</td>
				<td >
					<a href="/inscription/edit/{{_id}}"><i class="fas fa-pencil-alt"></i></a>
				</td>
				<td >
					<form action="/proyects/delete/{{_id}}" method="post">
						<button type="submit" class="btn btn-danger btn-sm">Borrar</button>
					</form>
				</td>
			</tr>
			</tbody>

		{{/each}}
	</table>
	<a href="/proyects/formproyect" class="btn btn-success">Ingresar Nuevo Proyecto</a>
</div>
